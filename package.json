{
  "name": "fortistate",
  "version": "3.0.0",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./useStore": {
      "types": "./dist/useStore.d.ts",
      "import": "./dist/useStore.js",
      "require": "./dist/useStore.js",
      "default": "./dist/useStore.js"
    },
    "./storeFactory": {
      "types": "./dist/storeFactory.d.ts",
      "import": "./dist/storeFactory.js",
      "require": "./dist/storeFactory.js",
      "default": "./dist/storeFactory.js"
    },
    "./ontogenesis": {
      "types": "./dist/ontogenesis/index.d.ts",
      "import": "./dist/ontogenesis/index.js",
      "require": "./dist/ontogenesis/index.js",
      "default": "./dist/ontogenesis/index.js"
    },
    "./physics": {
      "types": "./dist/physics/index.d.ts",
      "import": "./dist/physics/index.js",
      "require": "./dist/physics/index.js",
      "default": "./dist/physics/index.js"
    },
    "./inspector": {
      "types": "./dist/inspector.d.ts",
      "import": "./dist/inspector.js",
      "require": "./dist/inspector.js",
      "default": "./dist/inspector.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "src",
    "docs",
    "examples"
  ],
  "scripts": {
  "build": "tsc",
    "dev": "tsc -w",
  "test": "vitest run",
    "test:watch": "vitest --watch",
    "smoke": "node test/smoke.mjs",
    "plugins-smoke": "npm run build && node test/plugins-smoke.mjs",
    "smoke:example:react": "node examples/react-minimal/smoke.js",
    "smoke:example:vue": "node examples/vue-minimal/smoke.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm test",
    "cli": "node --loader ts-node/esm src/cli.ts",
    "inspect": "node dist/cli.js inspect",
    "inspect:dev": "node ./scripts/inspect-dev.mjs",
    "start:examples": "echo \"See examples/ for per-example start instructions\"",
    "init": "node ./scripts/init-example.mjs",
    "lint": "echo 'lint placeholder'",
  "ai:train": "cross-env TS_NODE_PROJECT=tsconfig.scripts.json node --loader ts-node/esm ./scripts/train-agents.ts",
    "precheck": "node ./scripts/pre-publish-check.mjs",
    "release": "npm run precheck && npm publish",
    "release:dry": "npm run precheck && npm publish --dry-run"
  },
  "keywords": [
    "state",
    "state-management",
    "inspector",
    "devtools",
    "react",
    "vue",
    "collaboration",
    "debugging",
    "real-time",
    "snapshot",
    "fortistate",
    "ontogenesis",
    "physics",
    "vscode-extension"
  ],
  "author": "axfrgo <noreply@users.noreply.github.com>",
  "license": "ISC",
  "description": "ðŸš€ A powerful state management library with collaborative inspector, real-time debugging, store snapshots, and IDE integration. Features include share snapshots, locate stores in code, and team collaboration tools.",
  "repository": {
    "type": "git",
    "url": "https://github.com/axfrgo/fortistate.git"
  },
  "bugs": {
    "url": "https://github.com/axfrgo/fortistate/issues"
  },
  "homepage": "https://github.com/axfrgo/fortistate#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.14",
    "@types/ws": "^8.18.1",
    "cross-env": "^10.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.2",
    "vitest": "^1.0.0",
    "ws": "^8.13.0"
  },
  "type": "module",
  "bin": {
    "fortistate": "dist/cli.js"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "jsonwebtoken": "^9.0.0",
    "react": "^19.1.1"
  }
}
